<odoo>
    <record id="action_logistics_shipment" model="ir.actions.act_window">
        <field name="name">Shipments</field>
        <field name="res_model">logistics.shipment</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="logistics_shipment_sequence" model="ir.sequence">
        <field name="name">Shipment Sequence</field>
        <field name="code">logistics.shipment</field>
